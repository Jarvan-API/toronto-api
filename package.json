{
  "name": "toronto-backend",
  "version": "1.0.0",
  "description": "NestJS server project for Toronto",
  "author": "federico.pedraza4@gmail.com",
  "private": true,
  "license": "UNLICENSED",
  "main": "src/main.ts",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm_src": "typeorm-ts-node-commonjs -d src/infrastructure/config/typeorm/typeorm.config.ts",
    "migrate": "ts-node node_modules/.bin/typeorm migration:run",
    "migration:revert": "typeorm-ts-node-commonjs -d src/infrastructure/config/typeorm/typeorm.config.ts migration:revert",
    "migration:run": "typeorm-ts-node-commonjs -d src/infrastructure/config/typeorm/typeorm.config.ts migration:run",
    "postinstall": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@nestjs/axios": "3.0.0",
    "@nestjs/common": "10.0.0",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.0.0",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "10.1.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/swagger": "7.1.13",
    "@nestjs/throttler": "5.0.0",
    "@nestjs/websockets": "^10.3.0",
    "@sentry/node": "7.84.0",
    "@sentry/profiling-node": "1.2.6",
    "@sentry/tracing": "7.84.0",
    "async": "3.2.4",
    "axios": "1.5.1",
    "bcrypt": "5.1.1",
    "cassandra-driver": "^4.7.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-mongo": "^5.1.0",
    "dayjs": "1.11.10",
    "dotenv": "16.3.1",
    "eventemitter2": "^6.4.9",
    "express-session": "1.17.3",
    "helmet": "7.0.0",
    "jwks-rsa": "3.1.0",
    "jwt-decode": "3.1.2",
    "mongodb": "^5.9.2",
    "mongoose": "^8.1.0",
    "morgan": "1.10.0",
    "otp-generator": "4.0.1",
    "passport": "0.6.0",
    "passport-custom": "1.1.1",
    "passport-local": "1.0.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "7.8.1",
    "socket.io-client": "^4.7.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.0.0",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/schematics": "10.0.0",
    "@nestjs/testing": "10.0.0",
    "@types/async": "3.2.22",
    "@types/axios": "0.14.0",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.17",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.2",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "1.4.10",
    "@types/node": "20.3.1",
    "@types/passport-local": "^1.0.36",
    "@types/password": "^0.1.2",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "jest": "^29.5.0",
    "jest-module-name-mapper": "^0.1.5",
    "lint-staged": "15.2.0",
    "prettier": "3.0.0",
    "prettier-eslint": "15.0.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "swagger-ui-express": "5.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix",
      "prettier --config ./.prettierrc --write"
    ],
    "**/*.{md,json}": [
      "prettier --config ./.prettierrc --write"
    ]
  }
}
